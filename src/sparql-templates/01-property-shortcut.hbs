name: insert
inputs:
    - name: "result"
    - name: "predicate0"
    - name: "predicate1"

{{#each prefixes}}
    PREFIX {{this.prefix}}: <{{this.iri}}>
{{/each}}
INSERT {
    ?start {{result}} ?end .
}
WHERE {
    ?start  {{predicate0}} ?middle .
    ?middle {{predicate1}} ?end    .
}
---
name: delete
inputs:
    - name: "predicate0"
    - name: "predicate1"

{{#each prefixes}}
    PREFIX {{this.prefix}}: <{{this.iri}}>
{{/each}}
DELETE WHERE {
    ?start  {{predicate0}} ?middle .
    ?middle {{predicate1}} ?end    .
}
