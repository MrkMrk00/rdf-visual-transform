name: insert
inputs:
    - name: "shortcut"
    - name: "newSubject"
    - name: "predicate0"
    - name: "predicate1"

{{#each prefixes}}
    PREFIX {{{this.prefix}}}: <{{{this.iri}}}>
{{/each}}
INSERT {
    {{{newSubject}}} {{{predicate0}}} ?start .
    {{{newSubject}}} {{{predicate1}}} ?end   .
}
WHERE {
    ?start {{{shortcut}}} ?end .
};
