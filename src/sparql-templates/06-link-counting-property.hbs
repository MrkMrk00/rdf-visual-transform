name: insert

INSERT {
    ?x {{{newProperty}}} ?count .
}
WHERE {
    {
        SELECT ?x (COUNT(?y) AS ?count)
        WHERE {
            ?x ?p ?y .
            FILTER(?p = {{{sourceProperty}}})
        }
        GROUP BY ?x
    }
}
---
name: delete

DELETE {
    ?x ?p ?y .
}
WHERE {
    ?x ?p ?y .
    FILTER(?p = {{{sourceProperty}}})
}
